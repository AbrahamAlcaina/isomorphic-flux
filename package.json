    "babel-loader": "^5.3.2",
    "body-parser": "^1.13.2",
    "bundle-loader": "~0.5.4",
    "classnames": "^2.1.3",
    "compression": "^1.5.1",
    "cookie-parser": "^1.3.5",
    "css-loader": "^0.15.5",
    "csurf": "^1.8.3",
    "debug": "^2.2.0",
    "eslint": "^1.0.0-rc-1",
    "express": "^4.13.1",
    "express-state": "^1.2.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "fastclick": "^1.0.6",
    "fluxible": "^0.5.1",
