{
  "name": "e1",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "webpack-dev": "webpack --progress --colors --watch",
    "webpack-pro": "node node_modules/webpack/bin/webpack.js -p --config webpack.config.pro.js",
    "nodemon": "nodemon --exec babel-node -- src/server/server.js",
    "babel-pro": "babel src --out-dir lib",
    "dev": "npm run nodemon & npm run webpack-dev",
    "clean": "rm -rf public & rm -rf lib",
    "postinstall": "npm run clean & npm run babel-pro & npm run webpack-pro",
    "start": "node lib/server/server.js",
    "lint": "eslint src"
  },
  "engines": {
    "node": "0.12.0",
    "npm": "2.10.x"
  },
  "dependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.6.7",
    "babel-eslint": "^3.1.18",
    "babel-loader": "^5.1.4",
    "body-parser": "^1.13.1",
    "bundle-loader": "~0.5.4",
    "compression": "^1.5.0",
    "cookie-parser": "^1.3.5",
    "css-loader": "^0.14.5",
    "csurf": "^1.8.3",
    "debug": "^2.2.0",
    "eslint": "^0.23.0",
    "express": "^4.13.0",
    "express-state": "^1.2.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "fluxible": "^0.4.12",
    "fluxible-plugin-fetchr": "^0.3.3",
    "fluxible-router": "^0.1.9",
    "isomorphic-fetch": "^2.1.0",
    "json-loader": "~0.5.2",
    "mongoose": "^4.0.6",
    "node-sass": "^3.2.0",
    "nodemon": "^1.3.7",
    "react": "^0.13.3",
    "sass-loader": "^1.0.2",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.12.3",
    "superagent": "^1.2.0",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  },
